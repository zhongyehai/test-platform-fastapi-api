# MySQL 配置，必填
MYSQL_HOST=
MYSQL_PORT=
MYSQL_USER=
MYSQL_PASSWORD=
MYSQL_DATABASE=

# Token 配置，必填
AUTH_SECRET_KEY=xxxxxx
AUTH_ACCESS_TOKEN_TIME_OUT=3600
AUTH_REFRESH_TOKEN_TIME_OUT=604800
# 密码加密的字符串，一旦生成用户，不可更改，否则两次加密密文会不一致
AUTH_PASSWORD_SECRET_KEY=xxxxxx
# 身份验证机制 SSO 或者 test_platform
AUTH_TYPE=test_platform

# 服务器运行配置，必填
MAIN_PORT=8018
MAIN_HOST=http://localhost
JOB_PORT=8019
JOB_HOST=http://localhost

# Webhook 配置，选填
DEFAULT_WEBHOOK_TYPE=
DEFAULT_WEBHOOK_ADDR=
WEBHOOK_SECRET=

# SSO 配置，选填
SSO_HOST_LINUX=
SSO_HOST_DEV=
SSO_AUTH_ENDPOINT=
SSO_TOKEN_ENDPOINT=
SSO_CLIENT_ID_LINUX=
SSO_CLIENT_ID_DEV=
SSO_CLIENT_SECRET_LINUX=
SSO_CLIENT_SECRET_DEV=
SSO_REDIRECT_URI_LINUX=
SSO_REDIRECT_URI_DEV=
